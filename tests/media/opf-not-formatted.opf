    <dc:title>La maison d'Hadès</dc:title>
    <dc:creator>Rick Riordan</dc:creator>
    <dc:language>French</dc:language>
    <dc:date>2014</dc:date>
    <dc:publisher>Albin Michel</dc:publisher>
